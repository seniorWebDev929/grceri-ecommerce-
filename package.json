{
  "name": "grceri",
  "version": "1.54.2",
  "license": "MIT",
  "scripts": {
    "local": "npm run build:local && npm run serve:ssr",
    "prod": "npm run build:prod && npm run serve:ssr",
    "sass-lint": "sass-lint -c sass-lint.yml -v -q",
    "ts-lint": "ng lint --type-check --force",
    "webpack:server": "webpack --mode development --config ./server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:local": "ng run grceri:build:local && ng run grceri:server && npm run webpack:server",
    "build:prod": "ng run grceri:build:production --build-optimizer=true && ng run grceri:server:production && npm run webpack:server",
    "build:stats": "ng build --prod --stats-json",
    "analyze": "webpack-bundle-analyzer dist/browser/stats.json",
    "lint": "npm run sass-lint && npm run ts-lint",
    "mobile": "ng run grceri:mobile:build --base-href ./ --output-path ../mobile/www/",
    "mobile-remove": "rmdir /s /q ..\\mobile\\www\\assets\\sitemap"
  },
  "private": true,
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
