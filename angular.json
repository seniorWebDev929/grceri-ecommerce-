{
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "grceri": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./config/browser.config.js",
              "replaceDuplicatePlugins": true
            },
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.browser.ts",
            "tsConfig": "src/tsconfig.browser.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/assets/favicon/",
                "output": "/"
              },
              "src/robots.txt",
              "src/ads.txt",
              "src/sitemap.xml",
              "src/manifest.json"
            ],
            "styles": [
              "src/assets/css/base/_loader.scss",
              "./node_modules/ngx-bar-rating/themes/br-horizontal-theme.css",
              "./node_modules/ngx-bar-rating/themes/br-fontawesome-o-theme.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/font-awesome.css",
              "src/assets/css/main.scss"
            ],
            "scripts": [
              "node_modules/chart.js/dist/Chart.min.js",
              "src/assets/js/annyang.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            },
            "local": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "grceri:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "grceri:build:production"
            }
          }
        },
        "mobile": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./config/mobile.config.js",
              "replaceDuplicatePlugins": true
            },
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.browser.ts",
            "tsConfig": "src/tsconfig.browser.json",
            "polyfills": "src/polyfills.ts",
            "styles": [
              "src/assets/css/base/_loader.scss",
              "./node_modules/ngx-bar-rating/themes/br-horizontal-theme.css",
              "./node_modules/ngx-bar-rating/themes/br-fontawesome-o-theme.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/font-awesome.css",
              "src/assets/css/main.scss"
            ],
            "scripts": [
              "node_modules/chart.js/dist/Chart.min.js",
              "src/assets/js/annyang.min.js"
            ]
          },
          "configurations": {
            "build": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.mobile.ts"
                }
              ]
            }
          }
        },
        "extract-i18n": {
          "builder": "wa/build-angular:extract-i18n",
          "options": {
            "browserTarget": "grceri:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "scripts": [
              "node_modules/chart.js/dist/Chart.min.js"
            ],
            "styles": [
              "src/assets/css/base/_loader.scss",
              "./node_modules/ngx-bar-rating/themes/br-horizontal-theme.css",
              "./node_modules/ngx-bar-rating/themes/br-fontawesome-o-theme.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/font-awesome.css",
              "src/assets/css/main.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src/assets/favicon/",
                "output": "/"
              },
              "src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "src/main.server.ts",
            "tsConfig": "src/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    },
    "grceri-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "grceri:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "grceri",
  "schematics": {
    "@schematics/angular:class": {
      "spec": false
    },
    "@schematics/angular:component": {
      "spec": false,
      "inlineStyle": true,
      "inlineTemplate": true,
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "spec": false,
      "prefix": "app"
    },
    "@schematics/angular:guard": {
      "spec": false
    },
    "@schematics/angular:module": {
      "spec": false
    },
    "@schematics/angular:pipe": {
      "spec": false
    },
    "@schematics/angular:service": {
      "spec": false
    }
  }
}